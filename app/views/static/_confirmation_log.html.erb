<% recent_audit_items.each do |audit_item| %>
<div class="homepage-block col-md-3">
  <h4><%= audit_item.user.full_name %></h4>
  <p><strong>Week Starting:</strong> <%= audit_item.start_date %></p>
	<% if audit_item.confirmed? %>
  	<p><strong>Confirmed:</strong> <%= audit_item.end_date %></p>
	<% end %>
	<p><strong>Status:</strong> <%= label_status audit_item.status %></p>
</div>
<% end %>
