<% employee_pending_confirmations.each do |employee_pending_confirmation| %>
	<%= link_to "I did not work any overtime for the week of: #{employee_pending_confirmation.start_date} through #{employee_pending_confirmation.start_date + 6.days}", confirm_audit_log_path(employee_pending_confirmation), class: 'btn btn-block btn-primary btn-lg btn-employee-home', id: "confirm_#{employee_pending_confirmation.id}", data: { confirm: 'Officially verify?' } %>
<% end %>